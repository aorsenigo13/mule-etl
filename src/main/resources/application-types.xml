<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="csv-example" format="csv">
      <types:shape format="weave" example="import.csv"><![CDATA[%dw 2.0

type csv_example = Array<{|  id: String,   nome: String,   cognome: String,   categoria: String,   livello: String,   compenso: Number,   cap: String,   citta: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
    <types:type name="derby-export" format="csv">
      <types:shape format="weave" example="import.csv"><![CDATA[%dw 2.0

type derby_export = Array<{|  nominativo: String,   occupazione: String,   livello: String,   cap: String,   residenza: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#55a77203-5099-403e-9042-25ebf0192ace">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="csv-example"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="derby-export"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>